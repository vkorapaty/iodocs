            },
            {
                "MethodName": "Remove group",
                "Synopsis": "Will remove the group listed. The sites themselves within the group will not be deleted. (From Group.pm:sub handle_delete) | stub",
                "HTTPMethod": "DELETE",
                "URI": "/:client_id/group/:group_id",
                "RequiresOAuth": "N",
                "parameters": [
                    {
                        "Name": "client_id",
                        "Required": "Y",
                        "Type": "integer",
                        "Description": "Client id"
                    },
                    {
                        "Name": "group_id",
                        "Required": "Y",
                        "Type": "integer",
                        "Description": "group_id of group to be deleted/removed."
                    }
                ]
            },
            {
                "MethodName": "Remove user privilege",
                "Synopsis": "Removes the given user's group privilege. | stub",
                "HTTPMethod": "DELETE",
                "URI": "/:client_id/group/:group_id/role/:target_user_id",
                "RequiresOAuth": "N",
                "parameters": [
                    {
                        "Name": "client_id",
                        "Required": "Y",
                        "Type": "integer",
                        "Description": "Client id"
                    },
                    {
                        "Name": "group_id",
                        "Required": "Y",
                        "Type": "integer",
                        "Description": "Group from which user will be removed."
                    },
                    {
                        "Name": "target_user_id",
                        "Required": "Y",
                        "Type": "integer",
                        "Description": "user_id of user to be deleted/removed."
                    }
                ]
            },
